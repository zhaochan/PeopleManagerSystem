<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.levilee.store.entity">
	<class name="Keynumber"  table="keynumber">
		<id name="keyname" type="string" column="keyname">
			<generator class="assigned"></generator>
		</id>
		<property name="keynumber" type="int" column="keynumber"></property>
	</class>
</hibernate-mapping>