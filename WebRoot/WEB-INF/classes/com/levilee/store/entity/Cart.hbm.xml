<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.levilee.store.entity">
	<class name="Cart"  table="cart">
		<id name="cartid" type="int" column="cartid">
			<generator class="assigned"></generator>
		</id>
		<property name="productid" type="int" column="productid"></property>
		<property name="amount" type="int" column="amount"></property>
		<property name="username" type="string" column="username"></property>
	</class>
</hibernate-mapping>