<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.levilee.store.entity">
	<class name="Collection"  table="collection">
		<id name="collectionid" type="int" column="collectionid">
			<generator class="assigned"></generator>
		</id>
		<property name="username" type="string" column="username"></property>
		<property name="productid" type="int" column="productid"></property>
		<property name="date" type="date" column="date"></property>
	</class>
	
</hibernate-mapping>