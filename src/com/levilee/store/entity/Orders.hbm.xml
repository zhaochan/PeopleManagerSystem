<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.levilee.store.entity">
	<class name="Orders"  table="orders">
		<id name="orderid" type="int" column="orderid">
			<generator class="assigned"></generator>
		</id>
		<property name="username" type="string" column="username"></property>
		<property name="productid" type="int" column="productid"></property>
		<property name="orderdate" type="date" column="orderdate"></property>
		<property name="status" type="int" column="status"></property>
	</class>
</hibernate-mapping>