<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.levilee.store.entity">
	<class name="Categories"  table="categories">
		<id name="categoryid" type="int" column="categoryid">
			<generator class="assigned"></generator>
		</id>
		<property name="categoryname" type="string" column="categoryname"></property>
	</class>
</hibernate-mapping>